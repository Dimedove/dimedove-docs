{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Dimedove",
  "logo": {
    "dark": "/logo/dimedove-logo-white.png",
    "light": "/logo/dimedove-logo.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#16a34a",
    "light": "#07C983",
    "dark": "#16a34a",
    "anchors": {
      "from": "#0D9373",
      "to": "#16a34a"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@dimedove.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Go to app",
    "url": "https://dimedove.com",
    "style": "pill",
    "arrow": false
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://mintlify.com/docs"
    },
    {
      "name": "Community",
      "icon": "slack",
      "url": "https://mintlify.com/community"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://mintlify.com/blog"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "linkedin": "https://linkedin.com/company/dimedove"
  },
  "font": {
    "headings": {
      "family": "Inter"
    },
    "body": {
      "family": "Inter"
    }
  },
  "modeToggle": {
    "default": "light",
    "isHidden": true
  },
  "layout": "topnav",
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "seo": {
    "indexHiddenPages": true
  }
}

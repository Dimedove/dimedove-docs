{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Dimedove",
  "description": "Comprehensive documentation for Dimedove's customer engagement platform",
  "appearance": {
    "default": "system",
    "strict": false
  },
  "styling": {
    "codeblocks": "system",
    "eyebrows": "section"
  },
  "icons": {
    "library": "lucide"
  },
  "fonts": {
    "body": {
      "family": "Inter"
    },
    "heading": {
      "family": "Inter"
    }
  },
  "colors": {
    "primary": "#0f0e0d",
    "light": "#f4f4f5",
    "dark": "#27272a"
  },
  "favicon": "/favicon.ico",
